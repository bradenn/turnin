<div class="row">

    <div class="col-12">
        <% if(typeof backName != "undefined"){ %>
            <a href="<%= backLink %>"><i class="fas fa-chevron-left"></i> <%= backName %></a>
        <% } %>
        <h1><%- title %></h1>
        <div class="d-flex flex-row justify-content-between align-items-center">
            <span class="sub-header"><%- subtitle %></span>
            <% buttons.forEach(button => { %>
                <%- button %>
            <% }); %>
        </div>
        <hr>
    </div>
</div>

